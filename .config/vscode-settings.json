{
    "remove-tabs-on-save.ignoreFileExtensions": ["*.go"],
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "monospace",
    "files.exclude": {
        "**/.DS_Store": false,
        "**/.git": false,
        "**/.hg": false,
        "**/.svn": false,
        "**/CVS": false,
        "**/Thumbs.db": false
    },
    "files.associations": {
        "*.rasi": "rasi"
    },
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "files.defaultLanguage": "shell",
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "workbench.enableExperiments": false,
    "workbench.welcomePage.extraAnnouncements": false,
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.preferredDarkColorTheme": "One Dark Pro Darker",
    "workbench.preferredLightColorTheme": "Visual Studio Light",
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editor.highlightModifiedTabs": true,
    "window.autoDetectColorScheme": false,
    "window.autoDetectHighContrast": false,
    "window.commandCenter": true,
    "window.titleBarStyle": "custom",
    "window.titleSeparator": " | ",
    "extensions.closeExtensionDetailsOnViewChange": true,
    "extensions.experimental.useUtilityProcess": true,
    "terminal.external.linuxExec": "kitty",
    "terminal.integrated.customGlyphs": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.ignoreProcessNames": [
        "starship",
        "oh-my-posh",
        "bash",
        "zsh",
        "atuin"
    ],
    // "terminal.integrated.defaultProfile.linux": "bash",
    // "terminal.integrated.profiles.linux": {
    //     "bash": {
    //         "path": "/usr/bin/flatpak-spawn",
    //         "args": ["--host", "--env=TERM=xterm-256color", "bash"]
    //     }
    // },

    "security.workspace.trust.enabled": false,
    "oneDarkPro.vivid": true,
    "vscord.app.name": "VSCodium",
    "vscord.status.problems.text": "{problems_count} skill issues",
    "vscord.behavoiur.prioritizeLanguagesOverExtensions": true,
    "svelte.enable-ts-plugin": true,
    "files.insertFinalNewline": true,
    "prettier.semi": false,
    "prettier.tabWidth": 4,
    "git.enableSmartCommit": true,
    "editor.guides.highlightActiveIndentation": "always",
    "html.format.indentInnerHtml": true,
    "editor.stickyTabStops": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "vscord.status.details.text.editing": "{problems}",
    "vscord.status.details.text.viewing": "{problems}",
    "vscord.status.state.text.idle": "Touching grass",
    "vscord.status.state.text.notInFile": "Doing jack shit",
    "vscord.status.state.text.notInWorkspace": "Hardly working",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": true,
    "git.openRepositoryInParentFolders": "never",
    "vscord.status.state.text.editing": "Breaking {file_name}{file_extension} [{current_line}:{current_column}]",
    "vscord.status.image.large.debugging.text": "Fixing a {LANG} file",
    "vscord.status.image.large.editing.text": "Breaking a {LANG} file",
    "vscord.status.image.large.notInFile.text": "Doing jack shit",
    "vscord.status.image.small.debugging.text": "Abusing {app_name}",
    "vscord.status.buttons.button1.idle.label": "Hardly working",
    "vscord.status.buttons.button1.idle.url": "https://github.com/REALERvolker1",
    "vscord.status.buttons.button1.inactive.enabled": true,
    "vscord.status.buttons.button1.inactive.label": "Most active button ever",
    "vscord.status.buttons.button1.inactive.url": "https://motherfuckingwebsite.com",
    "vscord.status.buttons.button1.idle.enabled": true,
    "vscord.status.buttons.button1.active.label": "Follow me on github ðŸš€ðŸ¤—",
    "vscord.status.buttons.button1.active.enabled": true,
    "vscord.status.buttons.button1.active.url": "https://github.com/REALERvolker1",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explicitFolding.rules": {
        "*": {
            "begin": "{{{",
            "end": "}}}"
        }
    },
    "color-highlight.markerType": "outline",
    "color-highlight.markRuler": false,
    "editor.formatOnPaste": true,
    // "python.formatting.provider": "none",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.smoothScrolling": true,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "shellformat.flag": "-i 4",
    "[properties]": {
        "editor.defaultFormatter": null
    },
    "vscord.behaviour.suppressNotifications": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "readme",
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "evenBetterToml.formatter.allowedBlankLines": 2,
    "evenBetterToml.formatter.arrayTrailingComma": true,
    "evenBetterToml.formatter.arrayAutoExpand": true,
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "[perl]": {
        "editor.defaultFormatter": "richterger.perl"
    },
    "python.autoComplete.extraPaths": [
        "/home/vlk/.vscode-oss/extensions/continue.continue-0.8.1-linux-x64"
    ],
    "diffEditor.codeLens": true,
    "continue.telemetryEnabled": false,
    "python.languageServer": "Pylance",
    "telemetry.telemetryLevel": "off",
    "code-runner.enableAppInsights": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.mode": "manual",
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "python.experiments.optOutFrom": ["All"],
    "workbench.commandPalette.experimental.suggestCommands": true,
    "python.experiments.enabled": false,
    "python.experiments.optInto": [],
    "html.format.enable": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "ccls.cache.directory": ".cache/ccls",
    "perl.cacheDir": "${workspace}/.cache/perl-lang",
    "bashIde.backgroundAnalysisMaxFiles": 0,
    "shellformat.effectLanguages": [
        "shellscript",
        "dockerfile",
        "dotenv",
        "hosts",
        "jvmoptions",
        "ignore",
        "gitignore",
        "properties",
        "spring-boot-properties",
        "azcli"
    ],
    "vscord.status.idle.enabled": false,
    "python.analysis.extraPaths": [
        "/home/vlk/.vscode-oss/extensions/continue.continue-0.8.1-linux-x64"
    ],
    "window.zoomLevel": -1,
    "oneDarkPro.bold": true,
    "[lua]": {
        "editor.defaultFormatter": "JohnnyMorganz.stylua"
    },
    "stylua.configPath": "~/.config/stylua/stylua.toml",
    "stylua.styluaPath": "/usr/bin/stylua",
    "editor.copyWithSyntaxHighlighting": false,
    "rust-analyzer.showUnlinkedFileNotification": false,
    "rust-analyzer.hover.actions.references.enable": true,
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
    "rust-analyzer.inlayHints.rangeExclusiveHints.enable": true,
    "rust-analyzer.lru.capacity": 256,
    "codeium.aggressiveShutdown": true,
    "[c]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    },
    "go.diagnostic.vulncheck": "Imports"
}
