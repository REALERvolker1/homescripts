; default theme, also embedded in the source of fast-syntax-highlighting

[base]
default          = none
unknown-token    = 9,bold
commandseparator = 250
redirection      = 250
here-string-tri  = yellow
here-string-text = 18
here-string-var  = 227,bg:18
exec-descriptor  = 227,bold
comment          = 240,bold
correct-subtle   = 12
incorrect-subtle = 1
subtle-separator = 2
subtle-bg        = bg:18
secondary        = free
; recursive-base   =

[command-point]
reserved-word  = yellow
subcommand     = yellow
alias          = 49
suffix-alias   = 49
global-alias   = 49,bg:17
builtin        = 49,underline
function       = 51
command        = 51,bold
precommand     = 51
hashed-command = 51
single-sq-bracket = 83
double-sq-bracket = 83,bold
double-paren   = 83

[paths]
path          = 120
pathseparator = 120,bold
path-to-dir   = 120,underline
globbing      = 120,bold
globbing-ext  = 13

[brackets]
paired-bracket = bg:blue
bracket-level-1 = green,bold
bracket-level-2 = yellow,bold
bracket-level-3 = cyan,bold

[arguments]
single-hyphen-option   = 39
double-hyphen-option   = 39
back-quoted-argument   = none
single-quoted-argument = 84
double-quoted-argument = 84
dollar-quoted-argument = 227

[in-string]
; backslash in $'...'
back-dollar-quoted-argument           = 227
; backslash or $... in "..."
back-or-dollar-double-quoted-argument = 227

[other]
variable             = 113
assign               = 227
assign-array-bracket = 226
history-expansion    = blue,bold

[math]
mathvar = blue,bold
mathnum = magenta
matherr = red

[for-loop]
forvar = none
fornum = magenta
; operator
foroper = yellow
; separator
forsep = yellow,bold

[case]
case-input       = green
case-parentheses = yellow
case-condition   = blue
